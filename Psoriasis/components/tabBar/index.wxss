.tabBar {
    font-family: PingFangSC-Regular, PingFang SC;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    display: flex;
    align-items: center;
    justify-content: space-around;
    width: 100%;
    height: 98rpx;
}

.tabBar .tabBar_item {
    background: #FFFFFF;
    height: 100%;
    flex: 1;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: space-around;
}

.tabBar .tabBar_item .icon {
    width: 50rpx;
    height: 50rpx;
}

.tabBar .tabBar_item .font {
    font-size: 19rpx;
    color: #999999;
}

.tabBar .tabBar_item .active {
    color: #0A9E67 !important;
}
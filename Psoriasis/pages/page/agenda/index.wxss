.container {
  font-family: PingFangSC-Medium, PingFang SC;
}

.container .nav {
  width: 100%;
  height: 100rpx;
  display: flex;
  box-shadow: 5rpx 1rpx 27rpx 10rpx rgba(0, 0, 0, 0.05);
  margin-bottom: 40rpx;
}

.container .nav_v {
  position: relative;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}

.container .nav_active {
  color: #333333 !important;
}

.container .nav_title {
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  width: 112px;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #999999;
  text-shadow: 0px 1rpx 27rpx rgba(0, 0, 0, 0.05);
}

.container .nav_hr {
  position: absolute;
  bottom: 0%;
  left: auto;
  width: 120rpx;
  height: 6rpx;
  background: #0A9E67;
  box-shadow: 0px 1rpx 27rpx 0px rgba(0, 0, 0, 0.05);
  border-radius: 3rpx;
}

.container .agenda_box {
  margin: 0 30rpx;
  position: relative;
  min-width: 672rpx;
  box-sizing: border-box;
  border-left: 2rpx dashed #cecece;
  min-height: 223rpx;
  padding-bottom: 50rpx;
}

.container .agenda_box .dot {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  top: -6rpx;
  left: -15rpx;
}

.container .agenda_box .agenda_item {
  margin-left: 30rpx;
  width: 628rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: space-around;
  box-shadow: 2px 1px 20px rgba(0, 0, 0, 0.1);
  border-radius: 32rpx;
}

.container .agenda_box .topTips {
  margin: 0 auto;
  width: 628rpx;
  min-height: 141rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  justify-content: space-between;
  padding: 15rpx 0rpx;
  text-align: justify;
}

.container .agenda_box .topTips .time_box {
  /* flex: 1; */
  position: relative;
  /* min-width: 200rpx; */
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 32rpx;
  height: 100%;
}

.container .agenda_box .topTips .time_box .heightline {
  width: 4rpx;
  height: 48rpx;
  background: #0A9E67;
  border-radius: 2rpx;
  position: absolute;
  top: auto;
  left: 0;
}

.container .agenda_box .topTips .time_box .timeFont {
  font-size: 24rpx;
  color: #999999;
  margin-bottom: 4rpx;
}

.container .agenda_box .topTips .time_box .time {
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
}

.container .agenda_box .topTips .interval {
  width: 1px;
  height: 61rpx;
  border-left: 1px dashed #d4d4d4;
  margin: 0 30rpx;
}

.container .agenda_box .topTips .rightBox {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-right: 32rpx;
}

.container .agenda_box .topTips .rightBox .conferenceTips {
  /* min-width: 150px;
  max-width: 150rpx; */
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  flex: 1;
}

.container .agenda_box .topTips .rightBox .top {
  margin-left: 10rpx;
  width: 32rpx;
  height: 32rpx;
}

.container .agenda_box .agendaInfo {
  margin: 0 auto;
  width: 632rpx;
  border-radius: 32rpx;
  box-sizing: border-box;
  padding: 0rpx 30rpx 30rpx 30rpx;
  font-size: 26rpx;
  color: #666666;

}

.container .agenda_box .agendaInfo .agendaFont {
  font-size: 26rpx;
  color: #666666;
}